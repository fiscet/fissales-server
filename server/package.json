{
  "name": "fissales-server",
  "version": "1.0.0",
  "description": "AI-powered sales agents with Mastra integration and Express API server",
  "main": "server.ts",
  "type": "module",
  "scripts": {
    "start": "node dist/server.js",
    "start:dev": "node --loader tsx server.ts",
    "dev": "tsx watch server.ts",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist/ node_modules/.cache/"
  },
  "keywords": [
    "ai",
    "sales",
    "agents",
    "mastra",
    "shopify",
    "firebase",
    "express",
    "typescript",
    "multi-agent"
  ],
  "author": "Christian Zanchetta",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/openai": "^1.0.0",
    "@mastra/core": "^0.16.0",
    "@mastra/libsql": "^0.14.0",
    "@mastra/loggers": "^0.10.11",
    "@mastra/memory": "^0.14.4",
    "@mastra/qdrant": "^0.11.10",
    "@mastra/rag": "^1.2.4",
    "ai": "^4.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "firebase-admin": "^12.0.0",
    "helmet": "^7.1.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "mastra": "^0.12.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fiscet/fissales.git"
  },
  "bugs": {
    "url": "https://github.com/fiscet/fissales/issues"
  },
  "homepage": "https://fiscet.it",
  "directories": {
    "lib": "dist",
    "test": "tests"
  }
}
